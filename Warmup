public class Warmup {
    public static void main(String[] args) {
        double x1 = 66.6;
        double x2 = -20.20;
        if (x1 == x2){
            System.out.println(x1 + " = " + x2);
        }else if (x1 > x2){
            System.out.println(x1);
        }else {
            System.out.println(x2);
        }
    }
}
